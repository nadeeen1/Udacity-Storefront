{
  "name": "storefront",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node build/index.js",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build --onSuccess \"node ./build/index.js\"",
    "build": "npx tsc && npx eslint . --fix",
    "lint": "npx eslint . --fix && eslint . --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "type": "npx tsc",
    "jasmine" : "jasmine",
    "testing": "npm run build && set ENV=test&& db-migrate --env test reset:test&& db-migrate --env test up:test&& npm run jasmine",
    "up": "db-migrate up",
    "reset": "db-migrate reset",
    "testdown": "db-migrate --env test reset:test",
    "testup": "db-migrate --env test up:test"
  },
  "author": "Nadin Ashraf",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^3.10.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.3",
    "@types/pg": "^7.14.11",
    "@types/prettier": "^2.7.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "db-migrate-plugin-typescript": "^2.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.8.4"
  }
}
